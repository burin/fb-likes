<div class="your_likes">
<h2>Your Likes</h2>
<% if @likes.empty? %>
	<p>No Likes to show :(</p>
<% else %>
	<% @likes.each do |like| %>
	<div class="like item">
		<div class="photo"><img src="http://graph.facebook.com/<%= like.id %>/picture" height="50" width="50"></div>
		<div class="details"><h4><%= like.name %></h4></div>
	</div>
	<% end %>
<% end %>
<p><a href="/friends" class="button">Back to Friends</a></p>
</div>